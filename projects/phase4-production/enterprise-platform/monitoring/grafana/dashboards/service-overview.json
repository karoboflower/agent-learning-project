{
  "dashboard": {
    "title": "Enterprise Platform - Service Overview",
    "tags": ["enterprise-platform", "overview"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Availability",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "avg(up{job=~\".*-service\"})",
            "legendFormat": "Availability",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 0.95, "color": "yellow"},
                {"value": 0.99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate (RPS)",
        "type": "graph",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (job) / sum(rate(http_requests_total[5m])) by (job)",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Error Rate"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Response Time (P95)",
        "type": "graph",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (job, le))",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Response Time"},
          {"format": "short"}
        ]
      },
      {
        "id": 5,
        "title": "Agent Executions",
        "type": "graph",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(agent_executions_total[5m])) by (status)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Executions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 6,
        "title": "Agent Execution Duration",
        "type": "graph",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "title": "Token Consumption Rate",
        "type": "graph",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(agent_tokens_consumed_total[5m])) by (model, token_type)",
            "legendFormat": "{{model}} - {{token_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Tokens/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Cost per Hour",
        "type": "graph",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(agent_cost_total[1h])) by (model)",
            "legendFormat": "{{model}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "currencyUSD", "label": "Cost/hour"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Task Queue Length",
        "type": "graph",
        "gridPos": {"x": 0, "y": 20, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "sum(task_queue_length) by (priority)",
            "legendFormat": "{{priority}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Queue Length"},
          {"format": "short"}
        ]
      },
      {
        "id": 10,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "gridPos": {"x": 8, "y": 20, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "sum(rate(cache_hits_total[5m])) / (sum(rate(cache_hits_total[5m])) + sum(rate(cache_misses_total[5m])))",
            "legendFormat": "Hit Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.7, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"x": 16, "y": 20, "w": 8, "h": 6},
        "targets": [
          {
            "expr": "sum(db_connections_active) by (database)",
            "legendFormat": "{{database}} - active",
            "refId": "A"
          },
          {
            "expr": "sum(db_connections_idle) by (database)",
            "legendFormat": "{{database}} - idle",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 12,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 26, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "avg(cpu_usage_percent) by (service)",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU %", "max": 100},
          {"format": "short"}
        ]
      },
      {
        "id": 13,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"x": 12, "y": 26, "w": 12, "h": 6},
        "targets": [
          {
            "expr": "sum(memory_usage_bytes{type=\"heap_inuse\"}) by (service)",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "bytes", "label": "Memory"},
          {"format": "short"}
        ]
      }
    ]
  }
}
